#menu
    background-color: alpha(white, 0.6)
    border-right: solid 3px alpha(black, 0.3)
    font-family: x 
    font-size: 8pt
    height 100vh
    width 260px
    box-sizing border-box
    padding-top: 90px
    padding-left: 30px
    position: fixed
    // top 0px
    left -100%
    transition: all 0.5s ease-out

    a
        display: block
        padding-top: 15px
    
#menu.active
    left 0
    